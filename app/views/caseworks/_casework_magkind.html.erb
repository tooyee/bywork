<table class="table table-sm table-striped">
 <thead class="thead-dark">
  <tr>
    <!-- <th scope="col"><div class="d-inline p-1">序</div></th> -->
    <th><p class="d-inline ">代码</p></th>
    <th><p class="d-inline ">名称</p></th>
    <th><p class="d-inline ">类型</p></th>
    <th><p class="d-none d-sm-inline float-right float-none">状态</p></th>
  </tr>
 </thead>

 <tbody>
  <% @casework.kinds.each do |kinds| %>
   <tr>
     <td>
       <div class="d-inline">
         <%= link_to kinds.code, edit_casework_kind_path(kinds.casework, kinds)  %>
       </div>
     </td>
     <!-- <td><div class="d-inline"><%= kinds.code %></div></td> -->
     <td><div class="d-inline"><%= kinds.name %></div></td>
    <td><div class="d-inline"><%= kinds.kindtype %></div></td>
     <td><div class="d-none d-sm-inline"><p class="text-right"><%= kinds.status %></p></div></td>

   </tr>
  <% end %>
 </tbody>

 <tfoot>
  <tr >


  </tr>
 </tfoot>
</table>

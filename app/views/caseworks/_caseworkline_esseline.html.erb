<table class="table table-sm table-striped">
 <thead class="thead-dark">
  <tr>
    <!-- <th scope="col"><div class="d-inline p-1">序</div></th> -->
    <th><p class="d-inline ">代码</p></th>
    <th><p class="d-inline ">资源</p></th>
    <th><p class="d-inline ">内容</p></th>
    <th><p class="d-inline ">数量</p></th>
    <th><p class="d-inline ">单位</p></th>
    <th><p class="d-inline ">说明</p></th>
    <th><p class="p-0 d-none d-sm-inline ">开始</p></th>
    <th><p class="p-0 d-none d-sm-inline ">结束</p></th>
    <th><p class="d-none d-sm-inline ">从哪</p></th>
    <th><p class="d-none d-sm-inline ">到哪</p></th>
    <th><p class="d-none d-sm-inline float-right float-none">状态</p></th>
  </tr>
 </thead>

 <tbody>
  <% @casework.caseworkesselines.each do |caseworkesselines| %>
   <tr>
     <td>
       <div class="d-inline">
         <%= link_to caseworkesselines.esseline.code, edit_casework_caseworkesseline_path(caseworkesselines.casework, caseworkesselines)  %>
       </div>
     </td>
     <td><div class="d-inline"><%= caseworkesselines.esse.name %></div></td>
     <td><div class="d-inline"><%= caseworkesselines.esseline.name %></div></td>
     <td><div class="d-inline"><p class="text-right"><%= caseworkesselines.quantity %></p></div></td>
     <td><div class="d-inline"><p class="text-right"><%= caseworkesselines.unit %></p></div></td>
     <td><div class="d-inline"><p class="text-right"><%= caseworkesselines.remarks %></p></div></td>

     <td><div class="d-none d-sm-inline"><p class="text-right"><%= caseworkesselines.begindate %></p></div></td>
     <td><div class="d-none d-sm-inline"><p class="text-right"><%= caseworkesselines.enddate %></p></div></td>
     <td><div class="d-none d-sm-inline"><p class="text-right"><%= caseworkesselines.fromwhscode %></p></div></td>
     <td><div class="d-none d-sm-inline"><p class="text-right"><%= caseworkesselines.towhscode %></p></div></td>
     <td><div class="d-none d-sm-inline"><p class="text-right"><%= caseworkesselines.status %></p></div></td>

   </tr>
  <% end %>
 </tbody>

 <tfoot>
  <tr >

  </tr>
 </tfoot>
</table>

<p id="notice"><%= notice %></p>

<h1>Transactionunits</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactionunits.each do |transactionunit| %>
      <tr>
        <td><%= transactionunit.code %></td>
        <td><%= transactionunit.name %></td>
        <td><%= link_to 'Show', transactionunit %></td>
        <td><%= link_to 'Edit', edit_transactionunit_path(transactionunit) %></td>
        <td><%= link_to 'Destroy', transactionunit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transactionunit', new_transactionunit_path %>
